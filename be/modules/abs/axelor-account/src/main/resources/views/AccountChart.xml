<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
    
    <grid name="account-chart-grid" title="Account Chart" model="com.axelor.apps.account.db.AccountChart">
        <field name="name"/>
        <field name="code" x-bind="{{code|unaccent|uppercase}}" />
        <field name="countryCode"/>
    </grid>
    
    <form name="account-chart-form" title="Account Chart" model="com.axelor.apps.account.db.AccountChart">
        <panel name="general" title="General">
	        <field name="name" />
	        <field name="code" x-bind="{{code|unaccent|uppercase}}" readonly="true"/>
	        <field name="countryCode" readonly="true"/>
        </panel>
    </form>
    
</object-views>
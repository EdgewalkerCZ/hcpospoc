<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
	
	<form name="app-contract-config-form" title="App contract" model="com.axelor.apps.base.db.AppContract" width="large">
		<toolbar>
		    <button name="new" title="New" hidden="true"/>
		    <button name="delete" title="Delete" hidden="true"/>
		    <button name="back" title="Back" hidden="true"/>
		    <button name="copy" title="Copy" hidden="true"/>
		    <button name="search" title="Search" hidden="true"/>
		</toolbar>
		
		<panel title="Advanced management">
			
			<field name="isUnchangableContract" colSpan="4" />
			<field name="isAmendmentManagement" colSpan="4" />
			<field name="isRenewalManagement" colSpan="4" />
			
			<field name="isInvoicingManagement" colSpan="4" />
			<field name="isConsumptionManagement" colSpan="4" />
			<field name="isAdditionalBenefitManagement" colSpan="4" />
			<field name="isConfigContract" colSpan="4" />
			
		</panel>
	</form>
	
	
</object-views>
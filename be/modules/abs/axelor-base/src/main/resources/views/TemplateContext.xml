<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
    
    <grid name="template-context-grid" title="Template Context" model="com.axelor.apps.base.db.TemplateContext">
        <field name="name"/>
        <field name="templateContextLineList" form-view="template-context-line-form" grid-view="template-context-line-grid"/>
    </grid>
    
    <form name="template-context-form" title="Template Context" model="com.axelor.apps.base.db.TemplateContext" width="large">
    	<panel name="main">
        	<field name="name"/>
        	<panel-related field="templateContextLineList" colSpan="12" form-view="template-context-line-form" grid-view="template-context-line-grid"/>
        </panel>
    </form>
    
</object-views>